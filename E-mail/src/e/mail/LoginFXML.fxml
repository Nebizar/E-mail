<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<GridPane id="GridPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="e.mail.LoginFXMLController" alignment="center" hgap="10" vgap="10">
    <padding>
        <Insets top="25" right="25" bottom="25" left="25"/>
    </padding>
    <children>
        <Label text="Login" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="1" fx:id= "label" />
        <TextField GridPane.columnIndex = "0" GridPane.rowIndex = "1" GridPane.columnSpan="1" fx:id="input" />
        <Button text="Log In" GridPane.columnIndex = "1" GridPane.rowIndex = "2" GridPane.columnSpan="1" fx:id="log" onAction = "#handleLog"/>
        <Button text="Cancel" GridPane.columnIndex = "0" GridPane.rowIndex = "2" GridPane.columnSpan="1"  fx:id="cancel" onAction = "#handleCancel"/>
    </children>
</GridPane>
